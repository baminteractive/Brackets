<% layout('boilerplate') -%>

<h1>Brackets</h1>
<form>
  Seeds: <input type="text" name="seeds">
  <input type="submit" value="Submit">
</form>
<div id="full-bracket">
	<div class="left">
		<div class="row1" data-row="1">
			<div class="double-bracket">
				<div class="single-bracket high">
					<div>
						<div class="button 1-1" data-placeindex="1-1">1</div>
					</div>
					<div class="seperator"></div>
					<div class="bottom">
						<div class="button 1-2" data-placeindex="1-2">4</div>
					</div>
				</div>
				<div class="single-bracket low">
					<div class="top">
						<div class="button 1-3"data-placeindex="1-3">2</div>
					</div>
					<div class="seperator"></div>
					<div class="">
						<div class="button 1-4"data-placeindex="1-4">3</div>
					</div>
				</div>
			</div>
		</div>
		<div class='row2'>
			<div class="single-bracket mid">
				<div class="top">
				<div class="button 2-1" data-placeindex="2-1"></div>
				</div>
				<div class="seperator"></div>
				<div class="bottom">
					<div class="button 2-2" data-placeindex="2-2"></div>
				</div>
			</div>
		</div>
	<div class="single-bracket champ">
		<div class="3-1" data-placeindex="3-1"></div>
	</div>
	</div><!-- left -->
	<div class="right" data-row="1">
		<div class="double-bracket">
		<div class="single-bracket high">
			<div>
				<div class="button 1-5" data-placeindex="1-5">1</div>
			</div>
			<div class="seperator"></div>
			<div class="bottom">
				<div class="button 1-6" data-placeindex="1-6">4</div>
			</div>
		</div>
		<div class="single-bracket low">
			<div class="top">
				<div class="button 1-7"data-placeindex="1-7">2</div>
			</div>
			<div class="seperator"></div>
			<div class="">
				<div class="button 1-8"data-placeindex="1-8">3</div>
			</div>
		</div>
	</div>
	<div class="single-bracket mid">
		<div class="top">
			<div class="button 2-3" data-placeindex="2-3"></div>
		</div>
		<div class="seperator"></div>
		<div class="bottom">
			<div class="button 2-4" data-placeindex="2-4"></div>
		</div>
	</div>
	<div class="single-bracket champ">
		<div class="3-2" data-placeindex="3-2"></div>
	</div>
	</div>
</div>
<script type="text/javascript" src="/js/teams.js"></script>